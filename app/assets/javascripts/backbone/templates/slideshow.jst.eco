<div class="carousel slide" id="carousel">
	<div class="carousel-inner">
		<% for pic in @pics: %>
		<div class="item">
			<img src="<%= pic %>">
		</div>
		<% end %>
	</div>
	<a class="left control" href="#carousel" data-slide="prev"><span>‹</span></a>
	<a class="right control" href="#carousel" data-slide="next"><span>›</span></a>
</div>
